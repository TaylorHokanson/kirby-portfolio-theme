{
    "name": "TaylorHokanson/kirby-portfolio",
    "type": "project",
    "description": "Free customizable portfolio theme for Kirby CMS.",
    "keywords": [
        "kirby",
        "cms",
        "template",
        "theme"
    ],
    "license": "MIT",
    "require": {
        "getkirby/cms": "^4.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "scripts": {
        "start": [
          "Composer\\Config::disableProcessTimeout",
          "@php -S localhost:8008 kirby/router.php"
        ]
    }
    {
    "minimum-stability": "dev"
    }
}
